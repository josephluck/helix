{
  "name": "helix-benchmark",
  "version": "1.0.0",
  "description": "Helix benchmark",
  "main": "./src/index.tsx",
  "scripts": {
    "dev": "budo ./src/index.tsx --live --pushstate --host=localhost --port=8888 --base=/ --css ./node_modules/bootstrap/dist/css/bootstrap.css -- -p [ tsify ]",
    "dist": "npm run browserify",
    "browserify": "browserify -p tsify ./src/index.tsx > dist/bundle.js",
    "minify": "uglifyjs ./dist/bundle.js --compress --mangle -o ./dist/bundle.min.js"
  },
  "author": "Joseph Luck",
  "license": "ISC",
  "devDependencies": {
    "budo": "^9.4.7"
  },
  "dependencies": {
    "@types/node": "^7.0.6",
    "bootstrap": "^3.3.7",
    "browserify": "^14.1.0",
    "tsify": "^3.0.1",
    "tslint-config-standard": "^4.0.0",
    "uglifyify": "^3.0.4",
    "uglifyjs": "^2.4.10"
  }
}
