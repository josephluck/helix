{
  "name": "helix-benchmark",
  "version": "1.0.0",
  "description": "Helix benchmark",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "budo ./src/index.js --live --pushstate --host=localhost --port=8888 --base=/ --css ./node_modules/bootstrap/dist/css/bootstrap.css -- -t [ babelify ]",
    "dist": "npm run browserify",
    "browserify": "browserify ./src/index.js -g yo-yoify -t babelify > dist/bundle.js",
    "minify": "uglifyjs ./dist/bundle.js --compress --mangle -o ./dist/bundle.min.js"
  },
  "author": "Joseph Luck",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babelify": "^7.3.0",
    "budo": "^9.4.7"
  },
  "dependencies": {
    "@types/node": "^7.0.5",
    "babel": "^6.23.0",
    "bootstrap": "^3.3.7",
    "browserify": "^14.1.0",
    "tslint-config-standard": "^4.0.0",
    "uglifyify": "^3.0.4",
    "uglifyjs": "^2.4.10",
    "yo-yoify": "^3.5.0"
  }
}
